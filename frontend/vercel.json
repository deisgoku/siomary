{
    "builds": [
        {
            "src": "/app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
}
